{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf4026"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"ls_dest_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_dest'"
		},
		"ls_source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_source'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://store4026.dfs.core.windows.net/"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azure4026.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AzureGLData"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminuser"
		},
		"ls_dest_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azure4026.database.windows.net"
		},
		"ls_dest_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AZureGLData"
		},
		"ls_dest_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminuser"
		},
		"ls_source_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost,14330"
		},
		"ls_source_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ultiproDev"
		},
		"ls_source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b090fc45-ce45-49f3-b5d8-5848cb149029/resourceGroups/Ben-RG/providers/Microsoft.Storage/storageAccounts/store4026"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf4026_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"ls_dest_password": {
						"value": "[parameters('ls_dest_password')]"
					},
					"ls_source_password": {
						"value": "[parameters('ls_source_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"ls_dest_properties_typeProperties_server": {
						"value": "[parameters('ls_dest_properties_typeProperties_server')]"
					},
					"ls_dest_properties_typeProperties_database": {
						"value": "[parameters('ls_dest_properties_typeProperties_database')]"
					},
					"ls_dest_properties_typeProperties_userName": {
						"value": "[parameters('ls_dest_properties_typeProperties_userName')]"
					},
					"ls_source_properties_typeProperties_server": {
						"value": "[parameters('ls_source_properties_typeProperties_server')]"
					},
					"ls_source_properties_typeProperties_database": {
						"value": "[parameters('ls_source_properties_typeProperties_database')]"
					},
					"ls_source_properties_typeProperties_userName": {
						"value": "[parameters('ls_source_properties_typeProperties_userName')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}